<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>


<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane fx:id="root"
          fx:controller="com.tminions.app.OrganizationLastUploadedScreen"
          styleClass="main-panel"
          xmlns:fx="http://javafx.com/fxml"
          hgap="10">

    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="10.0" />
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
        <ColumnConstraints hgrow="SOMETIMES" percentWidth="10.0" />
    </columnConstraints>

    <rowConstraints>
        <RowConstraints vgrow="SOMETIMES" percentHeight="20.0" valignment="BOTTOM"/>
        <RowConstraints vgrow="SOMETIMES" percentHeight="20.0" valignment="TOP"/>
        <RowConstraints vgrow="SOMETIMES" percentHeight="20.0" valignment="TOP"/>
        <RowConstraints vgrow="SOMETIMES" percentHeight="20.0" valignment="TOP"/>
        <RowConstraints vgrow="SOMETIMES" percentHeight="20.0" valignment="TOP"/>
        <RowConstraints vgrow="SOMETIMES" percentHeight="20.0" valignment="TOP"/>
    </rowConstraints>

    <Label text="Organization's Last Upload Time"
           GridPane.columnIndex="1"
           GridPane.columnSpan="2"
           GridPane.rowIndex="0"/>

    <ListView fx:id="lastUploadListView"
              GridPane.columnIndex="1"
              GridPane.columnSpan="2"
              GridPane.rowIndex="1"
              GridPane.rowSpan="4"/>

</GridPane >
